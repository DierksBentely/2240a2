package as2;

public class Rectangle extends GeoObject
{
	private double side1,side2;
	
	public Rectangle()
	{
		side1 = 1;
		side2 = 1;
	}
	
	public Rectangle(double side1, double side2)
	{
		this.side1 = side1;
		this.side2 = side2;
	}
	
	@Override
	public double getArea()
	{
		double area;
		
		area = side1*side2;
		
		return area;
	}
	
	@Override
	public double getPerimeter()
	{
		double perimeter;
		
		perimeter = (side1 + side2) *2;
		
		return perimeter;
	}

}
